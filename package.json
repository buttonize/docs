{
	"name": "buttonize-docs",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"prepare": "husky install",
		"env": "run-script-os",
		"env:windows": "env-cmd -f .env-cmd cmd /C \"ssocreds -p %AWS_PROFILE%\" && env-cmd -f .env-cmd",
		"env:default": "env-cmd -f .env-cmd bash -c \"ssocreds -p \\$AWS_PROFILE\" && env-cmd -f .env-cmd",
		"start": "npm run env -- sst start",
		"build": "npm run env -- sst build",
		"deploy": "npm run env -- sst deploy",
		"remove": "npm run env -- sst remove",
		"console": "npm run env -- sst console",
		"typecheck": "tsc --noEmit",
		"test": "sst bind -- vitest run",
		"lint": "eslint 'stacks' 'docs' --quiet --ext .ts,.tsx,.js,.jsx,.json,.md,.mdx",
		"lint:fix": "eslint 'stacks' 'docs' --quiet --ext .ts,.tsx,.js,.jsx,.json,.md,.mdx --fix"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,json}": "eslint --quiet --fix",
		"*.{graphql,gql,html}": "prettier --write"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@purple/serverless-git-branch-stage-plugin": "^1.3.2",
		"@tsconfig/node18": "^18.2.2",
		"@types/node": "^18.11.9",
		"@typescript-eslint/eslint-plugin": "^6.18.1",
		"@typescript-eslint/parser": "^6.18.1",
		"aws-cdk-lib": "2.110.1",
		"env-cmd": "^10.1.0",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-astro": "0.31.0",
		"eslint-plugin-mdx": "^2.3.2",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-prettier": "5.1.3",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"husky": "8.0.1",
		"lint-staged": "11.2.6",
		"prettier": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.6",
		"run-script-os": "^1.1.6",
		"sst": "2.39.2",
		"typescript": "^5.3.3",
		"vitest": "^0.25.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0"
	},
	"workspaces": [
		"services",
		"docs"
	]
}
